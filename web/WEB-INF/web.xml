<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>onlineorder</servlet-name>
        <servlet-class>onlineorder</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>onlineord</servlet-name>
        <servlet-class>onlineord</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>onlinebe</servlet-name>
        <servlet-class>onlinebe.onlinebe</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>online</servlet-name>
        <servlet-class>Ecommerce.online</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoginVerificationBE</servlet-name>
        <servlet-class>Ecommerce.LoginVerificationBE</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddCustBE</servlet-name>
        <servlet-class>Ecommerce.AddCustBE</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CustLog</servlet-name>
        <servlet-class>Ecommerce.CustLog</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CartBE</servlet-name>
        <servlet-class>Ecommerce.CartBE</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddprodBE</servlet-name>
        <servlet-class>Ecommerce.AddprodBE</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ViewcustBE</servlet-name>
        <servlet-class>Ecommerce.ViewcustBE</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>onlineorder</servlet-name>
        <url-pattern>/onlineorder</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>onlineord</servlet-name>
        <url-pattern>/onlineord</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>onlinebe</servlet-name>
        <url-pattern>/onlinebe</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>online</servlet-name>
        <url-pattern>/online</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginVerificationBE</servlet-name>
        <url-pattern>/LoginVerificationBE</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddCustBE</servlet-name>
        <url-pattern>/AddCustBE</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CustLog</servlet-name>
        <url-pattern>/CustLog</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CartBE</servlet-name>
        <url-pattern>/CartBE</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddprodBE</servlet-name>
        <url-pattern>/AddprodBE</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ViewcustBE</servlet-name>
        <url-pattern>/ViewcustBE</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
